  <!-- lazy load more cards -->
  <script>
    var displayLine=1;
    function showNextLine(totalLines)
    {
      displayLine++;
      $("#row"+displayLine).show("show");
      buttonData(totalLines);
    }

    function showPrevLine(totalLines)
    {
      $("#row"+displayLine).hide("show");
      displayLine--;

      buttonData(totalLines);
    }

    function buttonData(totalLines)
    {
      $("#rowMoreRows").show();
      $("#rowLessRows").show();
      if(displayLine == totalLines)
      {
      $("#rowMoreRows").hide();
      }
      if(displayLine == 1)
      {
      $("#rowLessRows").hide();
      }
    }
  </script>
