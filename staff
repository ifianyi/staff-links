          {
            tablePre=tablePre+"<th style='display:table-cell'>"+packages[i]+"</th>";
          }
          table=tablePre+"</tr></tbody><tbody>";
          for(i=0;i<data.packages["1"].length;i++)
          {
            
            tmpData=data.packages["1"][i];
            table=table+"<tr><td  style='display:table-cell'>"+tmpData.type+"</td>";
             for(j=1;j<packages.length;j++)
              {
                table=table+"<td  style='display:table-cell'>"+tmpData[j-1]+"</td>";
              }
            table=table+"</tr>";
          }
          $("#tabs-"+tbNumber+"1").html(table+"<tbody></table>");
         table=tablePre+"</tr></tbody><tbody>";
          for(i=0;i<data.packages["2"].length;i++)
          {
            
