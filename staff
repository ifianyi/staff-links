    function getUrlVars() {
    var vars = {};
    var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi,    
    function(m,key,value) {
      vars[key] = value;
    });
    return vars;
  }

function createDataTabs(data,tbNumber)
  {
    if(data.number > 1)
        {
          divData="<div id=\"tabs"+tbNumber+"\" style='width:103%'><ul><li><a href=\"#tabs-"+tbNumber+"1\">Package 1</a></li><li><a href=\"#tabs-"+tbNumber+"2\">Package 2</a></li></ul><div id=\"tabs-"+tbNumber+"1\" style='padding-left: 0px;'></div><div id=\"tabs-"+tbNumber+"2\" style='padding-left: 0px;'></div></div>";
            $("#tabs-"+tbNumber).html(divData);
            $( "#tabs"+tbNumber ).tabs();

